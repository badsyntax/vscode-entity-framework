{
  "name": "webview-ui",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "start": "npm run dev",
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "lint": "npm run lint:prettier && npm run lint:ts && npm run lint:eslint",
    "lint:ts": "tsc --noEmit",
    "lint:fix": "npm run lint:fix:prettier && npm run lint:eslint -- --fix",
    "lint:prettier": "prettier --check \"**/*.{js,json,svg,md,yml,css}\"",
    "lint:fix:prettier": "prettier --write '**/*.{js,json,svg,md,yml,css}'",
    "lint:eslint": "eslint src --ext .ts"
  },
  "dependencies": {
    "@vscode/webview-ui-toolkit": "^1.4.0",
    "d3": "^7.9.0",
    "mermaid": "^11.2.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-icons": "^4.7.1"
  },
  "devDependencies": {
    "@types/d3": "^7.4.3",
    "@types/react": "^18.3.8",
    "@types/react-dom": "^18.3.0",
    "@types/vscode-webview": "^1.57.5",
    "@typescript-eslint/eslint-plugin": "^5.47.1",
    "@typescript-eslint/parser": "^5.47.1",
    "@vitejs/plugin-react": "^4.3.1",
    "eslint": "^8.30.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-react": "^7.31.11",
    "prettier": "^3.3.3",
    "typescript": "^4.9.3",
    "vite": "^5.4.14"
  }
}
